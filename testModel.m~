function testModel(theta,spikes,temp)

    [p,dp,tvec] = getPoly(temp,3);
    linear = theta(2)*polyval(p,tvec) + theta(3)*polyval(dp,tvec);
    figure()
    plot(tvec,linear);
    title('Linearity');
    
    predict = theta(1) + sigmoid(linear,theta(3),theta(4),theta(5));
    spikeArray{1} = spikes;
    r = plotPSTH(spikeArray,50);
    hold on
    plot(tvec,predict);

end